{
  "name": "SvelteReactiveDebugger",
  "version": "0.1",
  "description": "Monitor svelte reactive statements",
  "devtools_page": "devtools/devtools.html",
  "web_accessible_resources": ["devtools/content.js"],
  "background": {
    "scripts": [
      "devtools/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}